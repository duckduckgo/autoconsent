{
  "manifest_version": 3,
  "name": "Autoconsent",
  "version": "2025.10.7",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.bundle.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "permissions": [
    "cookies",
    "tabs",
    "scripting",
    "storage",
    "webNavigation",
    "browsingData"
  ],
  "action": {
    "browser_style": true,
    "default_title": "Autoconsent",
    "default_popup": "popup.html"
  },
  "devtools_page": "devtools/background.html"
}