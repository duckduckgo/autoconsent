{
  "manifest_version": 3,
  "name": "Autoconsent",
  "version": "2026.2.13",
  "background": {
    "scripts": [
      "background.bundle.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.bundle.js"
      ],
      "match_origin_as_fallback": true,
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "permissions": [
    "cookies",
    "tabs",
    "scripting",
    "storage",
    "webNavigation",
    "browsingData"
  ],
  "action": {
    "default_title": "Autoconsent",
    "default_popup": "popup.html"
  },
  "devtools_page": "devtools/background.html"
}