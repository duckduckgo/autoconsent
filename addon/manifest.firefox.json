{
  "manifest_version": 2,
  "name": "Autoconsent",
  "version": "2025.9.20",
  "background": {
    "scripts": [
      "background.bundle.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.bundle.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "permissions": [
    "*://*/*",
    "tabs",
    "storage",
    "webNavigation"
  ],
  "page_action": {
    "browser_style": true,
    "default_title": "Autoconsent"
  }
}