{
    "$schema": "https://raw.githubusercontent.com/duckduckgo/autoconsent/refs/heads/main/rules/schema.json",
    "name": "truyo",
    "prehideSelectors": ["#truyo-consent-module"],
    "detectCmp": [{ "exists": "#truyo-cookieBarContent" }],
    "detectPopup": [{ "visible": "#truyo-consent-module" }],
    "optIn": [{ "click": "button#acceptAllCookieButton" }],
    "optOut": [{ "click": "button#declineAllCookieButton" }]
}
