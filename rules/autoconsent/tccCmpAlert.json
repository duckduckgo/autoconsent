{
    "$schema": "https://raw.githubusercontent.com/duckduckgo/autoconsent/refs/heads/main/rules/schema.json",
    "name": "tccCmpAlert",
    "vendorUrl": "https://unknown",
    "prehideSelectors": ["#tccCmpAlert"],
    "detectCmp": [
        {
            "exists": "#tccCmpAlert"
        }
    ],
    "detectPopup": [
        {
            "visible": "#tccCmpAlert"
        }
    ],
    "optIn": [
        {
            "waitForThenClick": "xpath///span[contains(.,'Accetta tutto')]"
        }
    ],
    "optOut": [
        {
            "waitForThenClick": "xpath///span[contains(.,'Accetta solo cookie necessari')]"
        }
    ]
}
