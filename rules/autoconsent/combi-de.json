{
  "name": "combi-de",
  "vendorUrl": "https://www.combi.de/",
  "runContext": {
    "urlPattern": "^https://(www)\\.combi\\.de"
  },
  "prehideSelectors": [
    ".modal.modal--cookie-notice"
  ],
  "detectCmp": [
    {
      "exists": ".modal.modal--cookie-notice"
    }
  ],
  "detectPopup": [
    {
      "visible": ".modal.modal--cookie-notice.open"
    }
  ],
  "optIn": [
    {
      "waitForThenClick": "#accept-consent-all"
    }
  ],
  "optOut": [
    {
      "waitForThenClick": "#accept-consent-necessary"
    }
  ]
}
