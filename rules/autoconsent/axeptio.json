{
    "name": "axeptio",
    "prehideSelectors": [".axeptio_widget, #axeptio_overlay, .ax-widget, .axeptio_mount, .axeptio_widget_wrapper"],
    "detectCmp": [
        { 
            "exists": ".ax-website-overlay" 
        }
    ],
    "detectPopup": [
        { 
            "visible": ".axeptio_widget" 
        }
    ],
    "optIn": [
        {
            "click": "button#axeptio_btn_acceptAll"
        }
    ],
    "optOut": [
        {
            "click": "button#axeptio_btn_dismiss" 
        },
        {
            "hide": [
                ".axeptio_widget"
            ]
        }
    ],
    "test": [
        {
            "eval": "document.cookie.includes('axeptio_authorized_vendors=%2C%2C')"
        }
    ]
  }