{
    "name": "axeptio",
    "prehideSelectors": [".axeptio_widget, .axeptio-widget--open, #axeptio_overlay, .ax-widget, .Widget__WidgetStyle-sc-zhn46e-1, .axeptio_mount, #cookieconsent:desc"],
    "detectCmp": [
        { 
            "exists": ".axeptio_widget" 
        }
    ],
    "detectPopup": [
        { 
            "visible": ".axeptio_widget" 
        }
    ],
    "optIn": [
        {
            "click": "button#axeptio_btn_acceptAll"
        }
    ],
    "optOut": [
        {
            "click": "button#axeptio_btn_dismiss" 
        },
        {
            "hide": [
                ".axeptio_widget"
            ]
        }
    ],
    "test": [
        {
            "eval": "document.cookie.includes('axeptio_authorized_vendors=%2C%2C')"
        }
    ]
  }