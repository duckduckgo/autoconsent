{
    "name": "cookiejs-modal",
    "vendorUrl": "https://unknown",
    "prehideSelectors": [".cookiejs-banner-wrapper"],
    "detectCmp": [
        {
            "exists": ".cookiejs-banner-wrapper.modal"
        }
    ],
    "detectPopup": [
        {
            "visible": ".cookiejs-banner-wrapper .cookiejs-banner-text"
        }
    ],
    "optIn": [
        {
            "waitForThenClick": ".cookiejs-banner-wrapper .cookiejs-banner-li:nth-of-type(1) button"
        }
    ],
    "optOut": [
        {
            "waitForThenClick": ".cookiejs-banner-wrapper .cookiejs-banner-li:nth-of-type(2) button"
        }
    ],
    "test": [
        {
            "cookieContains": "cookiejs_preferences"
        }
    ]
}
