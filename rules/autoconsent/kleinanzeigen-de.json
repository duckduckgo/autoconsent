{
    "name": "kleinanzeigen-de",
    "runContext": {
        "urlPattern": "^https?://(www\\.)?kleinanzeigen\\.de"
    },
    "prehideSelectors": ["#gdpr-banner-container"],
    "detectCmp": [
        {
            "exists": "#gdpr-banner-container #gdpr-banner [data-testid=gdpr-banner-decline-button]"
        }
    ],
    "detectPopup": [{ "visible": "#gdpr-banner-container #gdpr-banner [data-testid=gdpr-banner-decline-button]" }],
    "optIn": [
        {
            "click": "#gdpr-banner-container #gdpr-banner [data-testid=gdpr-banner-accept]"
        }
    ],
    "optOut": [
        {
            "click": "#gdpr-banner-container #gdpr-banner [data-testid=gdpr-banner-decline-button]"
        }
    ]
}
