{
    "name": "hema",
    "prehideSelectors": [".cookie-modal"],
    "detectCmp": [{ "visible": ".cookie-modal .cookie-accept-btn" }],
    "detectPopup": [{ "visible": ".cookie-modal .cookie-accept-btn" }],
    "optIn": [{ "waitForThenClick": ".cookie-modal .cookie-accept-btn" }],
    "optOut": [{ "waitForThenClick": ".cookie-modal .js-cookie-reject-btn" }],
    "test": [
        {
            "cookieMatch": "cookies_rejected=1"
        }
    ]
}
