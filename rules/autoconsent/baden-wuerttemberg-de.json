{
    "name": "baden-wuerttemberg.de",
    "prehideSelectors": [".cookie-alert.t-dark"],
    "detectCmp": [
        {
            "exists": ".cookie-alert.t-dark"
        }
    ],
    "detectPopup": [
        {
            "visible": ".cookie-alert.t-dark"
        }
    ],
    "optIn": [
        {
            "click": ".cookie-alert__form input:not([disabled]):not([checked])"
        },
        {
            "click": ".cookie-alert__button button"
        }
    ],
    "optOut": [
        {
            "if": {
                "exists": "#cookie-accept-necessary"
            },
            "then": [
                {
                    "click": "#cookie-accept-necessary"
                }
            ],
            "else": [
                {
                    "click": ".cookie-alert .checkbox__input:checked:not(:disabled)",
                    "all": true,
                    "optional": true
                },
                {
                    "click": ".cookie-alert__button button"
                }
            ]
        }
    ]
}
