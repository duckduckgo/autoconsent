{
    "name": "samsung.com",
    "runContext": {
        "urlPattern": "^https://www\\.samsung\\.com/"
      },
    "cosmetic": true,
    "prehideSelectors": ["div.cookie-bar"],
    "detectCmp": [{ "exists": "div.cookie-bar" }],
    "detectPopup": [{ "visible": "div.cookie-bar" }],
    "optIn": [{ "click": "div.cookie-bar__manage > a" }],
    "optOut": [{ "hide": "div.cookie-bar" }]
}
