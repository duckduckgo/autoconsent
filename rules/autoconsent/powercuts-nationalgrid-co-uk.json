{
  "name": "powercuts-nationalgrid-co-uk",
  "vendorUrl": "https://powercuts.nationalgrid.co.uk/",
  "runContext": {
    "urlPattern": "^https://powercuts\\.nationalgrid\\.co\\.uk"
  },
  "prehideSelectors": [
    "#cookie-notice-underlay",
    "#cookie-notice"
  ],
  "detectCmp": [
    {
      "exists": "#cookie-notice"
    }
  ],
  "detectPopup": [
    {
      "visible": "#cookie-notice"
    }
  ],
  "optIn": [
    {
      "waitForThenClick": "#cookie-notice > button[x-on:click='acceptAll']"
    }
  ],
  "optOut": [
    {
      "waitForThenClick": "#cookie-notice > button[x-on:click='accept']"
    }
  ]
}
