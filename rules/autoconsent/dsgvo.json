{
    "name": "WP DSGVO Tools",
    "vendorUrl": "https://wordpress.org/plugins/shapepress-dsgvo/",
    "prehideSelectors": [".sp-dsgvo"],
    "detectCmp": [{ "exists": ".sp-dsgvo.sp-dsgvo-popup-overlay" }],
    "detectPopup": [{ "visible": ".sp-dsgvo.sp-dsgvo-popup-overlay", "check": "any" }],
    "optIn": [{ "waitForThenClick": ".sp-dsgvo-privacy-btn-accept-all", "all": true }],
    "optOut": [{ "waitForThenClick": ".sp-dsgvo-privacy-btn-accept-nothing" }],
    "test": [{ "cookieContains": "sp_dsgvo_cookie_settings", "negated": true }]
}
