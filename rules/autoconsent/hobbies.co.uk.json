{
    "name": "hobbies.co.uk",
    "cosmetic": true,
    "runContext": {
        "urlPattern": "^https://(www\\.)?hobbies.co.uk"
    },
    "prehideSelectors": ["#cookie-modal"],
    "detectCmp": [
        {
            "exists": "#notice-cookie-block"
        }
    ],
    "detectPopup": [
        {
            "visible": "#notice-cookie-block"
        }
    ],
    "optIn": [
        {
            "waitForThenClick": "#btn-cookie-allow"
        }
    ],
    "optOut": [
        {
            "waitForThenClick": "#btn-cookie-manage"
        },
        {
            "click": "#notice-cookie-block input:checked",
            "all": true
        },
        {
            "click": "#btn-cookie-save"
        }
    ],
    "test": [
        {
            "cookieContains": "thbcookies"
        }
    ]
}
