{
    "$schema": "https://raw.githubusercontent.com/duckduckgo/autoconsent/refs/heads/main/rules/schema.json",
    "name": "ef-ccpa",
    "vendorUrl": "https://eforms.com/",
    "cosmetic": false,
    "runContext": {
        "urlPattern": "^https://(www\\.)?eforms\\.com"
    },
    "prehideSelectors": ["#ef-ccpa"],
    "detectCmp": [
        {
            "exists": "#ef-ccpa"
        }
    ],
    "detectPopup": [
        {
            "visible": "#ef-ccpa"
        }
    ],
    "optIn": [
        {
            "waitForThenClick": "#ef-button-ccpa-agree"
        }
    ],
    "optOut": [
        {
            "waitForThenClick": "#ef-button-ccpa-decline"
        }
    ]
}
