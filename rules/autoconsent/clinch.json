{
    "name": "clinch",
    "intermediate": false,
    "runContext": {
        "frame": false,
        "main": true
    },
    "prehideSelectors": [
        ".consent-modal[role=dialog]"
    ],
    "detectCmp": [
        {
            "exists": ".consent-modal[role=dialog]"
        }
    ],
    "detectPopup": [
        {
            "visible": ".consent-modal[role=dialog]"
        }
    ],
    "optIn": [
        {
            "click": "#consent_agree"
        }
    ],
    "optOut": [
        {
            "click": "#manage_cookie_preferences"
        },
        {
            "click": "#cookie_consent_preferences input:checked",
            "all": true,
            "optional": true
        },
        {
            "click": "#consent_save"
        }
    ],
    "test": [
        {
            "eval": "EVAL_CLINCH_0"
        }
    ]
}