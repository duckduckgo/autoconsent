{
  "name": "paypal.com",
  "prehideSelectors": ["#gdprCookieBanner"],
  "detectCmp": [{ "exists": "#gdprCookieBanner"}],
  "detectPopup": [{ "visible": "#gdprCookieContent_wrapper" }],
  "optIn": [{ "click": "#acceptAllButton" }],
  "optOut": [
      {
          "wait": 200
      },
      {
          "click": ".gdprCookieBanner_decline-button"
      }
  ],
  "test": [
    { "wait": 500 },
    { "eval": "EVAL_PAYPAL_0" }
  ]
}
