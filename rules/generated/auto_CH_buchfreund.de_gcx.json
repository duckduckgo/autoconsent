{
    "name": "auto_CH_buchfreund.de_gcx",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://www.buchfreund.de/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?buchfreund\\.de/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#cookie-bar > p:not([id]) > a:nth-child(3):not([id])"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#cookie-bar > p:not([id]) > a:nth-child(3):not([id])"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#cookie-bar > p:not([id]) > a:nth-child(3):not([id])",
            "comment": "Ablehnen"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#cookie-bar > p:not([id]) > a:nth-child(3):not([id])",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
