{
    "name": "auto_FR_pearl.fr_cfe",
    "vendorUrl": "https://www.pearl.fr/",
    "cosmetic": false,
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?pearl\\.fr/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div:not([id]) > div:nth-child(3):not([id]) > span:nth-child(3):not([id])"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div:not([id]) > div:nth-child(3):not([id]) > span:nth-child(3):not([id])"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div:not([id]) > div:nth-child(3):not([id]) > span:nth-child(3):not([id])",
            "comment": "Continuer sans accepter"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div:not([id]) > div:nth-child(3):not([id]) > span:nth-child(3):not([id])",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
