{
    "name": "auto_CA_oip.manual.canon_o3l",
    "cosmetic": false,
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?oip\\.manual\\.canon/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#cookie_banner > div:nth-child(2):not([id]) > button:nth-child(2)#cookie_discent"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#cookie_banner > div:nth-child(2):not([id]) > button:nth-child(2)#cookie_discent"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#cookie_banner > div:nth-child(2):not([id]) > button:nth-child(2)#cookie_discent",
            "comment": "Reject"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#cookie_banner > div:nth-child(2):not([id]) > button:nth-child(2)#cookie_discent",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
