{
    "name": "auto_DE_autoampel.de_liu",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://www.autoampel.de/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?autoampel\\.de/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#cookies > div:not([id]) > button:nth-child(4)#consent-no"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#cookies > div:not([id]) > button:nth-child(4)#consent-no"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#cookies > div:not([id]) > button:nth-child(4)#consent-no",
            "comment": "Nur notwendige Cookies erlauben"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#cookies > div:not([id]) > button:nth-child(4)#consent-no",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
