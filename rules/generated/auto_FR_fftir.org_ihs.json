{
    "name": "auto_FR_fftir.org_ihs",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://www.fftir.org/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?fftir\\.org/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#cookie-law-info-bar > span:not([id]) > div:not([id]) > a:nth-child(2)#cookie_action_close_header_reject"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#cookie-law-info-bar > span:not([id]) > div:not([id]) > a:nth-child(2)#cookie_action_close_header_reject"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#cookie-law-info-bar > span:not([id]) > div:not([id]) > a:nth-child(2)#cookie_action_close_header_reject",
            "comment": "Refus"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#cookie-law-info-bar > span:not([id]) > div:not([id]) > a:nth-child(2)#cookie_action_close_header_reject",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
