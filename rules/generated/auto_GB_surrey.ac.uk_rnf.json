{
    "name": "auto_GB_surrey.ac.uk_rnf",
    "cosmetic": false,
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?surrey\\.ac\\.uk/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > dialog#cookie-consent-dialog > div:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2)#cookie-consent-reject-button"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > dialog#cookie-consent-dialog > div:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2)#cookie-consent-reject-button"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > dialog#cookie-consent-dialog > div:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2)#cookie-consent-reject-button",
            "comment": "Reject all"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > dialog#cookie-consent-dialog > div:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2)#cookie-consent-reject-button",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
