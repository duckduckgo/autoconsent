{
    "name": "auto_REGION_migrationobservatory.ox.ac.uk_h5s",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://migrationobservatory.ox.ac.uk/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?migrationobservatory\\.ox\\.ac\\.uk/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#rb_cookie_popup > div:not([id]) > a:nth-child(2)#cookie_custom"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#rb_cookie_popup > div:not([id]) > a:nth-child(2)#cookie_custom"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "waitForThenClick": "body > div#rb_cookie_popup > div:not([id]) > a:nth-child(2)#cookie_custom",
            "comment": "Customise"
        },
        {
            "waitForThenClick": "body > div#rb_cookie_popup > div:not([id]) > form:nth-child(4)#cookie_preference > dl:nth-child(1):not([id]) > dt:nth-child(3):not([id]) > label:nth-child(2):not([id]) > input[type=\"checkbox\"]:nth-child(1)#cookie_analytics",
            "comment": "ENABLE"
        },
        {
            "waitForThenClick": "body > div#rb_cookie_popup > div:not([id]) > form:nth-child(4)#cookie_preference > button:nth-child(2)#cookie_save",
            "comment": "Save Preferences"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#rb_cookie_popup > div:not([id]) > a:nth-child(2)#cookie_custom",
            "timeout": 1000,
            "check": "none"
        },
        {
            "waitForVisible": "body > div#rb_cookie_popup > div:not([id]) > form:nth-child(4)#cookie_preference > button:nth-child(2)#cookie_save",
            "timeout": 1000,
            "check": "none"
        }
    ]
}