{
    "name": "auto_GB_signin.ebay.co.uk_0",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "https://signin.ebay.co.uk/ws/eBayISAPI.dll?SignIn&ru=https%3A%2F%2Fmy.ebay.co.uk%2F&sgfl=sm&smuid=8224f427-6d26-11f0-b153-2b5468523676",
        "manuallyReviewUpdates": true
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?(signin\\.)?ebay\\.([.a-z]+)/"
    },
    "prehideSelectors": ["#gdpr-banner"],
    "detectCmp": [
        {
            "exists": "#gdpr-banner"
        }
    ],
    "detectPopup": [
        {
            "visible": "#gdpr-banner"
        }
    ],
    "optIn": [
        {
            "waitForThenClick": "#gdpr-banner-accept"
        }
    ],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "#gdpr-banner-decline",
            "comment": "Decline all"
        }
    ],
    "test": [
        {
            "waitForVisible": "#gdpr-banner-decline",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
