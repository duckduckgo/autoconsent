{
    "name": "auto_AU_orpheum.com.au_kdl",
    "cosmetic": false,
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?orpheum\\.com\\.au/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#__next > div:nth-child(7):not([id]) > div:nth-child(2):not([id]) > button:nth-child(1)#rcc-decline-button"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#__next > div:nth-child(7):not([id]) > div:nth-child(2):not([id]) > button:nth-child(1)#rcc-decline-button"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#__next > div:nth-child(7):not([id]) > div:nth-child(2):not([id]) > button:nth-child(1)#rcc-decline-button",
            "comment": "Decline all"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#__next > div:nth-child(7):not([id]) > div:nth-child(2):not([id]) > button:nth-child(1)#rcc-decline-button",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
