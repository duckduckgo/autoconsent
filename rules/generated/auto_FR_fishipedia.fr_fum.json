{
    "name": "auto_FR_fishipedia.fr_fum",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://www.fishipedia.fr/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?fishipedia\\.fr/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#cookies-banner > button:nth-child(2):not([id])"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#cookies-banner > button:nth-child(2):not([id])"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#cookies-banner > button:nth-child(2):not([id])",
            "comment": "Je refuse"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#cookies-banner > button:nth-child(2):not([id])",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
