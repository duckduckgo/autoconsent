{
    "name": "auto_GB_york.gov.uk_ot2",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://www.york.gov.uk/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?york\\.gov\\.uk/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#silktide-wrapper > div:nth-child(4)#silktide-banner > div:nth-child(2):not([id]) > button:nth-child(2):not([id])"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#silktide-wrapper > div:nth-child(4)#silktide-banner > div:nth-child(2):not([id]) > button:nth-child(2):not([id])"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#silktide-wrapper > div:nth-child(4)#silktide-banner > div:nth-child(2):not([id]) > button:nth-child(2):not([id])",
            "comment": "Reject non-essential"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#silktide-wrapper > div:nth-child(4)#silktide-banner > div:nth-child(2):not([id]) > button:nth-child(2):not([id])",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
