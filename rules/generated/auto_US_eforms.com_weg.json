{
    "name": "auto_US_eforms.com_weg",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://eforms.com/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?eforms\\.com/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#ef-ccpa > button:nth-child(3)#ef-button-ccpa-decline"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#ef-ccpa > button:nth-child(3)#ef-button-ccpa-decline"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#ef-ccpa > button:nth-child(3)#ef-button-ccpa-decline",
            "comment": "Decline"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#ef-ccpa > button:nth-child(3)#ef-button-ccpa-decline",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
