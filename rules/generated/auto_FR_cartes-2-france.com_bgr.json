{
    "name": "auto_FR_cartes-2-france.com_bgr",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://www.cartes-2-france.com/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?cartes-2-france\\.com/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#cookie-banner > div:nth-child(2):not([id]) > button:nth-child(2)#decline-cookies"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#cookie-banner > div:nth-child(2):not([id]) > button:nth-child(2)#decline-cookies"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#cookie-banner > div:nth-child(2):not([id]) > button:nth-child(2)#decline-cookies",
            "comment": "Refuser"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#cookie-banner > div:nth-child(2):not([id]) > button:nth-child(2)#decline-cookies",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
