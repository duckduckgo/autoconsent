{
    "name": "auto_FR_cultureindoor.com_rox",
    "vendorUrl": "https://www.cultureindoor.com/en/",
    "cosmetic": false,
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?cultureindoor\\.com/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div:not([id]) > div:nth-child(1):not([id]) > a:nth-child(1):not([id])"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div:not([id]) > div:nth-child(1):not([id]) > a:nth-child(1):not([id])"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div:not([id]) > div:nth-child(1):not([id]) > a:nth-child(1):not([id])",
            "comment": "Continue without accepting"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div:not([id]) > div:nth-child(1):not([id]) > a:nth-child(1):not([id])",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
