{
    "name": "auto_AU_hub.jw.org_urj",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "https://hub.jw.org/identity/api/login"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?login\\.jw\\.org/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div:not([id]) > section:not([id]) > div:nth-child(3):not([id]) > button:nth-child(2):not([id])"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div:not([id]) > section:not([id]) > div:nth-child(3):not([id]) > button:nth-child(2):not([id])"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div:not([id]) > section:not([id]) > div:nth-child(3):not([id]) > button:nth-child(2):not([id])",
            "comment": "Decline"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div:not([id]) > section:not([id]) > div:nth-child(3):not([id]) > button:nth-child(2):not([id])",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
