{
    "name": "auto_NL_zorgsaam.org_wxt",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://www.zorgsaam.org/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?zorgsaam\\.org/"
