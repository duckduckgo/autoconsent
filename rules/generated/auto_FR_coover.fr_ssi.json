{
    "name": "auto_FR_coover.fr_ssi",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://www.coover.fr/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?coover\\.fr/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#cookie-banner > div:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2)#adopt-reject-all-button"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#cookie-banner > div:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2)#adopt-reject-all-button"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#cookie-banner > div:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2)#adopt-reject-all-button",
            "comment": "Reject"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#cookie-banner > div:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2)#adopt-reject-all-button",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
