{
    "name": "auto_GB_fifecountry.co.uk_bdp",
    "vendorUrl": "https://www.fifecountry.co.uk/",
    "cosmetic": false,
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?fifecountry\\.co\\.uk/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#cookie-bar > a:nth-child(4):not([id])"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#cookie-bar > a:nth-child(4):not([id])"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#cookie-bar > a:nth-child(4):not([id])",
            "comment": "Decline All"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#cookie-bar > a:nth-child(4):not([id])",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
