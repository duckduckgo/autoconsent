{
    "name": "auto_NO_ao-universe.com_eey",
    "vendorUrl": "https://www.ao-universe.com/main/news",
    "cosmetic": false,
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?ao-universe\\.com/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#cookiebanner > a:nth-child(3):not([id])"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#cookiebanner > a:nth-child(3):not([id])"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#cookiebanner > a:nth-child(3):not([id])",
            "comment": "Reject cookies"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#cookiebanner > a:nth-child(3):not([id])",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
