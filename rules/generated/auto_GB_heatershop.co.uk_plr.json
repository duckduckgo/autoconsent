{
    "name": "auto_GB_heatershop.co.uk_plr",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://www.heatershop.co.uk/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?heatershop\\.co\\.uk/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div:not([id]) > div:nth-child(2):not([id]) > a:nth-child(1):not([id])"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div:not([id]) > div:nth-child(2):not([id]) > a:nth-child(1):not([id])"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div:not([id]) > div:nth-child(2):not([id]) > a:nth-child(1):not([id])",
            "comment": "Decline"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div:not([id]) > div:nth-child(2):not([id]) > a:nth-child(1):not([id])",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
