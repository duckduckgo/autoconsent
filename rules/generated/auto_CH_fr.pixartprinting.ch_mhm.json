    "_metadata": {
        "vendorUrl": "http://fr.pixartprinting.ch/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?fr\\.pixartprinting\\.ch/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#app > div:nth-child(1):not([id]) > div:nth-child(2):not([id]) > button:nth-child(1)#refuse-notif"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#app > div:nth-child(1):not([id]) > div:nth-child(2):not([id]) > button:nth-child(1)#refuse-notif"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#app > div:nth-child(1):not([id]) > div:nth-child(2):not([id]) > button:nth-child(1)#refuse-notif",
            "comment": "Refuser"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#app > div:nth-child(1):not([id]) > div:nth-child(2):not([id]) > button:nth-child(1)#refuse-notif",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
