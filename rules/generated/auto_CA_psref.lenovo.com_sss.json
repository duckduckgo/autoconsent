{
    "name": "auto_CA_psref.lenovo.com_sss",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://psref.lenovo.com/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?psref\\.lenovo\\.com/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#psref_cookie_consent_panel > div:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2)#psref_cookie_decline"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#psref_cookie_consent_panel > div:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2)#psref_cookie_decline"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#psref_cookie_consent_panel > div:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2)#psref_cookie_decline",
            "comment": "Decline"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#psref_cookie_consent_panel > div:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2)#psref_cookie_decline",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
