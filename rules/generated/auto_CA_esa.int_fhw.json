        },
        {
            "waitForThenClick": "body > div:not([id]) > div:nth-child(1)#cookie_loc > div:not([id]) > a:nth-child(3):not([id])",
            "comment": "ACCEPT ONLY ESSENTIAL COOKIES"
        }
