{
    "name": "auto_FR_sudoc.fr_xhf",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://www.sudoc.fr/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?sudoc\\.abes\\.fr/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#cookie-banner > a:nth-child(1):not([id])"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#cookie-banner > a:nth-child(1):not([id])"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#cookie-banner > a:nth-child(1):not([id])",
            "comment": "Refuser"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#cookie-banner > a:nth-child(1):not([id])",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
