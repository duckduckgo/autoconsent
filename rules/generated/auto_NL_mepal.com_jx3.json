{
    "name": "auto_NL_mepal.com_jx3",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://www.mepal.com/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?mepal\\.com/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#mepal-cb > div:nth-child(1)#mepal-cb-content > div:nth-child(3):not([id]) > a:nth-child(2)#DeclineAll"
        }
    ],
    "detectPopup": [
