{
    "name": "auto_FR_bdbase.fr_5x3",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://www.bdbase.fr/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?bdbase\\.fr/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#cookie-info > div:nth-child(2):not([id]) > button:nth-child(1):not([id])"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#cookie-info > div:nth-child(2):not([id]) > button:nth-child(1):not([id])"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#cookie-info > div:nth-child(2):not([id]) > button:nth-child(1):not([id])",
            "comment": "Refuser"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#cookie-info > div:nth-child(2):not([id]) > button:nth-child(1):not([id])",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
