{
    "name": "auto_NL_chillplanet.nl_xcp",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://chillplanet.nl/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?chillplanet\\.nl/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#cookie-popup > div:nth-child(2):not([id]) > button:nth-child(2)#reject-cookies"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#cookie-popup > div:nth-child(2):not([id]) > button:nth-child(2)#reject-cookies"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#cookie-popup > div:nth-child(2):not([id]) > button:nth-child(2)#reject-cookies",
            "comment": "Weigeren"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#cookie-popup > div:nth-child(2):not([id]) > button:nth-child(2)#reject-cookies",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
