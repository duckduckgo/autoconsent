{
    "name": "auto_AU_connect.mayoclinic.org_2ti",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://connect.mayoclinic.org/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?connect\\.mayoclinic\\.org/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#mayo-privacy-consent-banner > div:nth-child(3)#privacy-popup-controls-div > div:not([id]) > button:nth-child(2)#mayo-privacy-opt-out"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#mayo-privacy-consent-banner > div:nth-child(3)#privacy-popup-controls-div > div:not([id]) > button:nth-child(2)#mayo-privacy-opt-out"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#mayo-privacy-consent-banner > div:nth-child(3)#privacy-popup-controls-div > div:not([id]) > button:nth-child(2)#mayo-privacy-opt-out",
            "comment": "Reject additional cookies"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#mayo-privacy-consent-banner > div:nth-child(3)#privacy-popup-controls-div > div:not([id]) > button:nth-child(2)#mayo-privacy-opt-out",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
