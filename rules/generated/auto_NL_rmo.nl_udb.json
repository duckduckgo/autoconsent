{
    "name": "auto_NL_rmo.nl_udb",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://www.rmo.nl/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?rmo\\.nl/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#cookiebanner > div:nth-child(2)#c-right > a:nth-child(2):not([id])"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#cookiebanner > div:nth-child(2)#c-right > a:nth-child(2):not([id])"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#cookiebanner > div:nth-child(2)#c-right > a:nth-child(2):not([id])",
            "comment": "Alleen noodzakelijke cookies"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#cookiebanner > div:nth-child(2)#c-right > a:nth-child(2):not([id])",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
