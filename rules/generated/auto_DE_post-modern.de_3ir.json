{
    "name": "auto_DE_post-modern.de_3ir",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://www.post-modern.de/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?post-modern\\.de/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#abw_consentbanner > div:nth-child(4):not([id]) > button:nth-child(1)#abw_consent_deny"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#abw_consentbanner > div:nth-child(4):not([id]) > button:nth-child(1)#abw_consent_deny"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#abw_consentbanner > div:nth-child(4):not([id]) > button:nth-child(1)#abw_consent_deny",
            "comment": "Nur notwendige Cookies"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#abw_consentbanner > div:nth-child(4):not([id]) > button:nth-child(1)#abw_consent_deny",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
