{
    "name": "auto_DE_steuerbot.com_alm",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://www.steuerbot.com/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?steuerbot\\.com/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#radix-«R16» > div:nth-child(4):not([id]) > button:nth-child(2):not([id])"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#radix-«R16» > div:nth-child(4):not([id]) > button:nth-child(2):not([id])"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#radix-«R16» > div:nth-child(4):not([id]) > button:nth-child(2):not([id])",
            "comment": "Alle ablehnen"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#radix-«R16» > div:nth-child(4):not([id]) > button:nth-child(2):not([id])",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
