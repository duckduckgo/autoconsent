{
    "name": "auto_FR_carrelibertin.com_unm",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://www.carrelibertin.com/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?carrelibertin\\.com/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div:not([id]) > div:nth-child(3):not([id]) > a:nth-child(1)#refuseCookie"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div:not([id]) > div:nth-child(3):not([id]) > a:nth-child(1)#refuseCookie"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div:not([id]) > div:nth-child(3):not([id]) > a:nth-child(1)#refuseCookie",
            "comment": "Refuser"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div:not([id]) > div:nth-child(3):not([id]) > a:nth-child(1)#refuseCookie",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
