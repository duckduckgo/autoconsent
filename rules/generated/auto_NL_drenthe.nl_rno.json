{
    "name": "auto_NL_drenthe.nl_rno",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://www.drenthe.nl/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?drenthe\\.nl/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > dialog:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2):not([id])"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > dialog:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2):not([id])"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > dialog:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2):not([id])",
            "comment": "Alleen noodzakelijk"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > dialog:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2):not([id])",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
