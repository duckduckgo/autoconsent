{
    "name": "auto_AU_elekta.com_qht",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "https://www.elekta.com/company/careers/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?elekta\\.com/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#cookie-banner > p:nth-child(3):not([id]) > button:nth-child(2)#cookie-reject"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#cookie-banner > p:nth-child(3):not([id]) > button:nth-child(2)#cookie-reject"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#cookie-banner > p:nth-child(3):not([id]) > button:nth-child(2)#cookie-reject",
            "comment": "Reject all"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#cookie-banner > p:nth-child(3):not([id]) > button:nth-child(2)#cookie-reject",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
