{
    "name": "auto_NL_tennet.eu_xdm",
    "vendorUrl": "https://www.tennet.eu/",
    "cosmetic": false,
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?tennet\\.eu/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#__next > div:nth-child(5):not([id]) > div:nth-child(2):not([id]) > button:nth-child(1)#rcc-decline-button"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#__next > div:nth-child(5):not([id]) > div:nth-child(2):not([id]) > button:nth-child(1)#rcc-decline-button"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#__next > div:nth-child(5):not([id]) > div:nth-child(2):not([id]) > button:nth-child(1)#rcc-decline-button",
            "comment": "Accept necessary cookies"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#__next > div:nth-child(5):not([id]) > div:nth-child(2):not([id]) > button:nth-child(1)#rcc-decline-button",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
