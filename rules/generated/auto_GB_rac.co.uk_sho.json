{
    "name": "auto_GB_rac.co.uk_sho",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://www.rac.co.uk/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?rac\\.co\\.uk/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > section:not([id]) > div#ensNotifyBanner > div:nth-child(2):not([id]) > button:nth-child(2)#ensRejectAll[data-testid=\"ensNoEssentialOnlyBtn\"]"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > section:not([id]) > div#ensNotifyBanner > div:nth-child(2):not([id]) > button:nth-child(2)#ensRejectAll[data-testid=\"ensNoEssentialOnlyBtn\"]"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > section:not([id]) > div#ensNotifyBanner > div:nth-child(2):not([id]) > button:nth-child(2)#ensRejectAll[data-testid=\"ensNoEssentialOnlyBtn\"]",
            "comment": "Reject non-essential"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > section:not([id]) > div#ensNotifyBanner > div:nth-child(2):not([id]) > button:nth-child(2)#ensRejectAll[data-testid=\"ensNoEssentialOnlyBtn\"]",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
