    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?gotronic\\.fr/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div:not([id]) > form:nth-child(3):not([id]) > button:nth-child(4)#cookie_consent_use_only_functional_cookies"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div:not([id]) > form:nth-child(3):not([id]) > button:nth-child(4)#cookie_consent_use_only_functional_cookies"
        }
    ],
    "optIn": [],
