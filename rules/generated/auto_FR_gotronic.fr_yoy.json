        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div:not([id]) > form:nth-child(3):not([id]) > button:nth-child(4)#cookie_consent_use_only_functional_cookies",
            "comment": "Continuer sans accepter"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div:not([id]) > form:nth-child(3):not([id]) > button:nth-child(4)#cookie_consent_use_only_functional_cookies",
            "timeout": 1000,
            "check": "none"
        }
    ]
