{
    "name": "auto_DE_cosmos-indirekt.de_k8c",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://www.cosmos-indirekt.de/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?cosmos-indirekt\\.de/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#consent-banner > div:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2)#reject-all"
        }
    ],
    "detectPopup": [
