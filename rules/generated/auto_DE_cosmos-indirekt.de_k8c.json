            "visible": "body > div#consent-banner > div:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2)#reject-all"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#consent-banner > div:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2)#reject-all",
            "comment": "Nur notwendige"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#consent-banner > div:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2)#reject-all",
            "timeout": 1000,
            "check": "none"
        }
    ]
