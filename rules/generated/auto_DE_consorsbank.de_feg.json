{
    "name": "auto_DE_consorsbank.de_feg",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://www.consorsbank.de/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?consorsbank\\.de/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#tc-privacy-wrapper > div#popin_tc_privacy > div:nth-child(2)#popin_tc_privacy_container_button > button:nth-child(3)#essentialcookiesbutton"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#tc-privacy-wrapper > div#popin_tc_privacy > div:nth-child(2)#popin_tc_privacy_container_button > button:nth-child(3)#essentialcookiesbutton"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#tc-privacy-wrapper > div#popin_tc_privacy > div:nth-child(2)#popin_tc_privacy_container_button > button:nth-child(3)#essentialcookiesbutton",
            "comment": "Nur notwendige Cookies"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#tc-privacy-wrapper > div#popin_tc_privacy > div:nth-child(2)#popin_tc_privacy_container_button > button:nth-child(3)#essentialcookiesbutton",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
