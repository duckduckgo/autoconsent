{
    "name": "auto_NL_scholenopdekaart.nl_qdr",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://scholenopdekaart.nl/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?scholenopdekaart\\.nl/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#cookiescript_injected_wrapper > div#cookiescript_injected > div:nth-child(4)#cookiescript_buttons > div:nth-child(3)#cookiescript_reject"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#cookiescript_injected_wrapper > div#cookiescript_injected > div:nth-child(4)#cookiescript_buttons > div:nth-child(3)#cookiescript_reject"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#cookiescript_injected_wrapper > div#cookiescript_injected > div:nth-child(4)#cookiescript_buttons > div:nth-child(3)#cookiescript_reject",
            "comment": "ALLES AFWIJZEN"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#cookiescript_injected_wrapper > div#cookiescript_injected > div:nth-child(4)#cookiescript_buttons > div:nth-child(3)#cookiescript_reject",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
