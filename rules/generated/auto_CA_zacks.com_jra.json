{
    "name": "auto_CA_zacks.com_jra",
    "cosmetic": false,
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?zacks\\.com/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div:not([id]) > div:nth-child(3):not([id]) > button:nth-child(2)#deny_cookie"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div:not([id]) > div:nth-child(3):not([id]) > button:nth-child(2)#deny_cookie"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div:not([id]) > div:nth-child(3):not([id]) > button:nth-child(2)#deny_cookie",
            "comment": "Deny Optional"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div:not([id]) > div:nth-child(3):not([id]) > button:nth-child(2)#deny_cookie",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
