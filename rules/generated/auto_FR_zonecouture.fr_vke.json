{
    "name": "auto_FR_zonecouture.fr_vke",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://www.zonecouture.fr/",
        "manuallyReviewUpdates": true
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?zonecouture\\.fr/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "#ez-cookie-notification__decline"
        }
    ],
    "detectPopup": [
        {
            "visible": "#ez-cookie-notification__decline"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "#ez-cookie-notification__decline",
            "comment": "Refuser"
        }
    ],
    "test": [
        {
            "waitForVisible": "#ez-cookie-notification__decline",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
