{
    "name": "auto_NL_distrelec.nl_dzt",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://distrelec.nl/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?distrelec\\.nl/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > section:not([id]) > div#ensNotifyBanner > div:nth-child(1)#ensBannerDescription > div:nth-child(4):not([id]) > button:nth-child(2)#btn-accept-essential-cookies"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > section:not([id]) > div#ensNotifyBanner > div:nth-child(1)#ensBannerDescription > div:nth-child(4):not([id]) > button:nth-child(2)#btn-accept-essential-cookies"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > section:not([id]) > div#ensNotifyBanner > div:nth-child(1)#ensBannerDescription > div:nth-child(4):not([id]) > button:nth-child(2)#btn-accept-essential-cookies",
            "comment": "Only essential cookies"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > section:not([id]) > div#ensNotifyBanner > div:nth-child(1)#ensBannerDescription > div:nth-child(4):not([id]) > button:nth-child(2)#btn-accept-essential-cookies",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
