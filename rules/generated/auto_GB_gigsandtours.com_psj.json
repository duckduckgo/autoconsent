{
    "name": "auto_GB_gigsandtours.com_psj",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://www.gigsandtours.com/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?gigsandtours\\.com/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#seeGdprCookieConsent > div:not([id]) > p:nth-child(2):not([id]) > a:nth-child(2)#seeGdprReject"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#seeGdprCookieConsent > div:not([id]) > p:nth-child(2):not([id]) > a:nth-child(2)#seeGdprReject"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#seeGdprCookieConsent > div:not([id]) > p:nth-child(2):not([id]) > a:nth-child(2)#seeGdprReject",
            "comment": "Reject"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#seeGdprCookieConsent > div:not([id]) > p:nth-child(2):not([id]) > a:nth-child(2)#seeGdprReject",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
