{
    "name": "auto_CH_eventfrog.ch_pgw",
    "vendorUrl": "https://eventfrog.ch/en/home.html",
    "cosmetic": false,
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?eventfrog\\.ch/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#tf-modal-container > div#tf-modal-container__modal > div:nth-child(3):not([id]) > a:nth-child(1):not([id])"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#tf-modal-container > div#tf-modal-container__modal > div:nth-child(3):not([id]) > a:nth-child(1):not([id])"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#tf-modal-container > div#tf-modal-container__modal > div:nth-child(3):not([id]) > a:nth-child(1):not([id])",
            "comment": "Decline"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#tf-modal-container > div#tf-modal-container__modal > div:nth-child(3):not([id]) > a:nth-child(1):not([id])",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
