{
    "name": "auto_NO_webank.it_6rx",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://webank.it/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?webank\\.it/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#cookie_banner > div:not([id]) > button:nth-child(2):not([id])"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#cookie_banner > div:not([id]) > button:nth-child(2):not([id])"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#cookie_banner > div:not([id]) > button:nth-child(2):not([id])",
            "comment": "Continua senza accettare"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#cookie_banner > div:not([id]) > button:nth-child(2):not([id])",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
