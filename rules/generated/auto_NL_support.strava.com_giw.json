{
    "name": "auto_NL_support.strava.com_giw",
    "cosmetic": false,
    "_metadata": {
        "vendorUrl": "http://support.strava.com/"
    },
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?support\\.strava\\.com/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#stravaCookieBanner > div:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2):not([id])"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#stravaCookieBanner > div:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2):not([id])"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#stravaCookieBanner > div:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2):not([id])",
            "comment": "Reject"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#stravaCookieBanner > div:not([id]) > div:nth-child(2):not([id]) > button:nth-child(2):not([id])",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
