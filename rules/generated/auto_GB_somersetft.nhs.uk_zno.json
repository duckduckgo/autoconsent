{
    "name": "auto_GB_somersetft.nhs.uk_zno",
    "cosmetic": false,
    "runContext": {
        "main": true,
        "frame": false,
        "urlPattern": "^https?://(www\\.)?somersetft\\.nhs\\.uk/"
    },
    "prehideSelectors": [],
    "detectCmp": [
        {
            "exists": "body > div#consentPopup > button:nth-child(3)#consentReject"
        }
    ],
    "detectPopup": [
        {
            "visible": "body > div#consentPopup > button:nth-child(3)#consentReject"
        }
    ],
    "optIn": [],
    "optOut": [
        {
            "wait": 500
        },
        {
            "waitForThenClick": "body > div#consentPopup > button:nth-child(3)#consentReject",
            "comment": "Reject all"
        }
    ],
    "test": [
        {
            "waitForVisible": "body > div#consentPopup > button:nth-child(3)#consentReject",
            "timeout": 1000,
            "check": "none"
        }
    ]
}
