{
  "name": "support.roku.com",
  "prehideSelectors": [
    ".roku-cookie-consent"
  ],
  "detectCmp": [
    {
      "exists": ".roku-cookie-consent"
    }
  ],
  "detectPopup": [
    {
      "visible": ".roku-cookie-consent"
    }
  ],
  "optIn": [
    {
      "waitForThenClick": ".cc-allow"
    }
  ],
  "optOut": [
    {
      "waitForThenClick": ".cc-deny"
    }
  ]
}