{
  "name": "audi.com",
  "prehideSelectors": [
    "#ensModalWrapper"
  ],
  "detectCmp": [
    {
      "exists": "#ensModalWrapper"
    }
  ],
  "detectPopup": [
    {
      "visible": "#ensModalWrapper"
    }
  ],
  "optIn": [
    {
      "waitForThenClick": "#ensCancel"
    }
  ],
  "optOut": [
    {
      "waitForThenClick": "body:nth-child(2) > section:nth-child(5) > div > div:nth-child(4) > div:nth-child(5) > button:nth-child(3)"
    }
  ]
}