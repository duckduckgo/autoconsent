    "@rollup/plugin-typescript": "^4.0.0",
    "bulma": "^0.7.4",
    "ejs": "^2.6.1",
    "lit-html": "^1.0.0",
    "puppeteer": "^1.14.0",
    "puppeteer-cluster": "^0.15.2",
    "rollup": "^1.32.1",
    "typescript": "^3.8.3",
